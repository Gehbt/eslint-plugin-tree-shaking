{
  "name": "eslint-plugin-tree-shaking",
  "version": "0.0.0",
  "description": "Spots side-effects in module initialization that would interfere with tree-shaking",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Lukas Taegert",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha tests --recursive",
    "lint": "prettier-standard *.js lib/**/*.js tests/**/*.js",
    "precommit": "lint-staged",
    "commitmsg": "validate-commit-msg",
    "prepush": "npm test",
    "postmerge": "npm install && npm prune",
    "postrewrite": "npm install && npm prune"
  },
  "lint-staged": {
    "*.js": [
      "prettier-standard",
      "standard --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^4.0.1",
    "mocha": "^3.1.2",
    "prettier-standard": "^6.0.0",
    "standard": "^10.0.2",
    "validate-commit-msg": "^2.12.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "license": "MIT"
}
